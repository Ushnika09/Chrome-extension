{
  "manifest_version": 3,
  "name": "Tab Title Picker",
  "version": "1.0.0",
  "description": "Get the current tab title with one click",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icons-16.png",
      "48": "icons/icons-48.png",
      "128": "icons/icons-128.png"
    }
  },
  "icons": {
    "16": "icons/icons-16.png",
    "48": "icons/icons-48.png",
    "128": "icons/icons-128.png"
  }
}